<body>
  <img id="catFace" class="cat-face" alt="котик" />

  <div id="counter" class="counter">Счетчик: 0</div>

  <button id="angryBtn" class="button">Я злой &gt;.&lt;</button>

  <div class="hint">
    нажми, если кот тебя разозлил, чтобы обменять кошки на вкусняшки
  </div>

  <button id="resetBtn" class="reset">Очистить</button>

  <script>
    const catFace = document.getElementById('catFace');
    const counter = document.getElementById('counter');
    const angryBtn = document.getElementById('angryBtn');
    const resetBtn = document.getElementById('resetBtn');

    // ВСТАВЬ Base64 ДЛЯ ДОБРОГО И ЗЛОГО КОТИКА
    const happyCat = "data:image/png;base64,";
    const angryCat = "data:image/png;base64,";

    let count = 0;
    let isAngry = false;

    // Устанавливаем доброго котика при загрузке страницы
    window.addEventListener('load', () => {
      catFace.src = happyCat;
    });

    // Загружаем сохранённый счётчик при старте
    if (localStorage.getItem('catCount')) {
      count = parseInt(localStorage.getItem('catCount'), 10);
      counter.textContent = `Счетчик: ${count}`;
    }

    // Клик на кнопку "Я злой"
    angryBtn.addEventListener('click', () => {
      count++;
      counter.textContent = `Счетчик: ${count}`;
      localStorage.setItem('catCount', count); // сохраняем значение
      document.body.style.backgroundColor = '#ffb3b3';
      catFace.src = angryCat;
      isAngry = true;
      setTimeout(() => {
        if (isAngry) {
          document.body.style.backgroundColor = '#fffacd';
          catFace.src = happyCat;
          isAngry = false;
        }
      }, 600);
    });

    // Очистка счётчика
    resetBtn.addEventListener('click', () => {
      count = 0;
      counter.textContent = `Счетчик: ${count}`;
      localStorage.removeItem('catCount'); // удаляем сохранённое значение
    });
  </script>
</body>
</html>

